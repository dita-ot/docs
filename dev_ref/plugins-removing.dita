<task id="plugins-removing" xml:lang="en">
  <title>Manually removing plug-ins</title>
  <shortdesc>Plug-ins can be installed by removing the plug-in and running integration process.</shortdesc>
  <taskbody>
    <steps>
      <step>
        <cmd>Remove plug-in installation directory.</cmd>
      </step>
      <step>
        <cmd>Run integration process.</cmd>
        <info>
          <codeblock>ant -f integrator.xml</codeblock>
        </info>
      </step>
    </steps>
  </taskbody>
</task>
